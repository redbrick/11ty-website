{
  "name": "@redbrick/11ty-website",
  "author": "Jed Hazaymeh <magma@redbrick.dcu.ie>",
  "version": "0.0.2",
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "clean": "rimraf dist",
    "dev:tailwind": "tailwindcss -i ./src/assets/main.css -o ./dist/assets/main.css --watch",
    "dev:eleventy": "ELEVENTY_ENV=development eleventy --serve",
    "dev": "npm-run-all clean --parallel \"dev:*\"",
    "build:tailwind": "tailwindcss -i ./src/assets/main.css -o ./dist/assets/main.css --minify",
    "build:eleventy": "ELEVENTY_ENV=development eleventy",
    "build": "NODE_ENV=production run-s clean \"build:*\""
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-fetch": "^4.0.0",
    "@redbrick/design-system": "https://github.com/redbrick/design-system",
    "@types/markdown-it": "^13.0.7",
    "debug": "^4.3.4",
    "eleventy-plugin-toc": "^1.1.5",
    "front-matter": "^4.0.2",
    "luxon": "^3.4.4",
    "markdown-it": "^14.0.0",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-replace-link": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "reading-time": "^1.5.0",
    "rimraf": "^5.0.5",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.4.1"
  }
}
